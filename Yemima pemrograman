# ARRAY 
mahasiswa_list = ["Ana", "Dewi", "Leha", "Maya", "Indriani"]

print("Daftar Nama Mahasiswa:", mahasiswa_list)

# Mengakses 
print("Mahasiswa pertama:", mahasiswa_list[0])

# Menambah 
mahasiswa_list.append("Aisyah")
print("Setelah menambah Aisyah:", mahasiswa_list)

# Menghapus 
mahasiswa_list.remove("Dewi")
print("Setelah menghapus Dewi:", mahasiswa_list)

# DICTIONARY 

mahasiswa = {
    "nama": "Yemima",
    "nim": "D0425307",
    "jurusan": "Sistem Informasi",
    "kelas": "Sisfo A",
    "angkatan": 2025,
    "alamat": "Mamasa, Sulawesi Barat",

    "nilai": {
        "algoritma_pemrograman": 98,
        "struktur_data": 87,
        "basis_data": 94,
        "pemrograman_python": 96
    },

    "mata_kuliah_diambil": [
        "Algoritma Pemrograman",
        "Basis Data",
        "Pemrograman Python",
        "Kalkulus Informatika"
    ]
}

# Menampilkan data
print("\nData Mahasiswa:", mahasiswa)

# Mengakses data
print("Nama:", mahasiswa["nama"])
print("Kelas:", mahasiswa["kelas"])
print("Nilai Python:", mahasiswa["nilai"]["pemrograman_python"])

# Update nilai
mahasiswa["nilai"]["pemrograman_python"] = 98
print("Nilai Python Setelah Update:", mahasiswa["nilai"]["pemrograman_python"])

# Menambah data baru
mahasiswa["status"] = "Aktif"
print("Setelah menambah status:", mahasiswa)
